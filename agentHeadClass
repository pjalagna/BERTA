# agentHeadClass.py
class AgentHead:
"""- 
pja - 05-24-2017 orig

usage 
include AgentHeadClass
m = AgentHeadClass.AgentHead(mark1,


parameters
    mark1 - code of tick.c to activate this agent
"""
    def __init__(self,mark1,Work,nextTick):
        ctl = 0
        while ( ctl == 0):
            j = SQRead1("select c from Tick;")
            if (j == "die"):
                ctl = -1 # break
            elif (j == mark1):
                Work()
                #next tick
            else:
                sleep(1500) # test 1500 , production 20
            #endif
        #wend


